{
  "manifest_version": 2,

  "name": "copy donwload links",
  "description": "This extension is used to copy all download links",
  "version": "1.0",


  "content_scripts": [{
    "matches": ["*://*/*"],
    "all_frames": true,
    "js": ["./scripts/jquery.js", "./scripts/content.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
